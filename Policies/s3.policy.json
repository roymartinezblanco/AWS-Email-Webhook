{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSESPuts",
            "Effect": "Allow",
            "Principal": {
                "Service": "ses.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::ak-activation-email/*",
            "Condition": {
                "StringEquals": {
                    "aws:Referer": "446474567375"
                }
            }
        }
    ]
}
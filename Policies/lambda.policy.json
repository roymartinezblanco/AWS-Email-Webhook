{
    "Version": "2012-10-17",
    "Id": "default",
    "Statement": [
      {
        "Sid": "allowSesInvoke",
        "Effect": "Allow",
        "Principal": {
          "Service": "ses.amazonaws.com"
        },
        "Action": "lambda:InvokeFunction",
        "Resource": "arn:aws:lambda:<changeme>",
        "Condition": {
          "StringEquals": {
            "AWS:SourceAccount": "446474567375"
          }
        }
      }
    ]
  }